<ul class="list">
    {{#each this}}


    <li class="list-item">


        <div class="photo-card">

            <img id='img' class="card-image" src="{{this.webformatURL}}" alt="{{this.tag}}"
                data-source="{{this.largeImageURL}}">


            <div class="stats">
                <p class="stats-item">
                    <i class="material-icons">thumb_up</i>
                    1108
                </p>
                <p class="stats-item">
                    <i class="material-icons">visibility</i>
                    320321
                </p>
                <p class="stats-item">
                    <i class="material-icons">comment</i>
                    129
                </p>
                <p class="stats-item">
                    <i class="material-icons">cloud_download</i>
                    176019
                </p>
            </div>

        </div>


    </li>

    {{/each}}
</ul>